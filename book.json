{
    "title": "知世说博客",
    "author": "zss team",
    "plugins": [
        "-lunr",
        "-search",
        "-sharing",
        "-livereload",
        "-fontsettings",
        "prism",
        "-highlight",
        "back-to-top-button",
        "chapter-fold",
        "expandable-chapters-small",
        "copy-code-button",
        "splitter",
        "search-pro",
        "pageview-count",
        "tbfed-pagefooter",
        "github"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/Urumuqi/gitbook"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy wuqi@知世说 2020",
            "modify_label": "article last edit time : ",
            "modify_format": "YYYY-MM-DD HH:mm"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ]
        }
    }
}