{
    "title": "知世说博客",
    "author": "ZSS's Team",
    "output.name": "ZSS's Blog",
    "plugins": [
        "-lunr",
        "-search",
        "-sharing",
        "-livereload",
        "-fontsettings",
        "prism",
        "-highlight",
        "back-to-top-button",
        "chapter-fold",
        "expandable-chapters-small",
        "copy-code-button",
        "splitter",
        "search-pro",
        "pageview-count",
        "tbfed-pagefooter",
        "github",
        "hide-element",
        "insert-logo",
        "popup"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/Urumuqi/gitbook"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy wuqi@知世说 2024",
            "modify_label": "article last edit at : ",
            "modify_format": "YYYY-MM-DD HH:mm"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ],
            "lang": {
                "json": "javascript"
            }
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "insert-logo": {
            "url": "",
            "style": ""
        }
    }
}